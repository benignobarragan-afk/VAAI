	function sacaPendientes() {

		var items = [];
		webix.ajax()
			.post("/api/op/op_cucsx2")
			.then(function (res) {
				if (res.json().length > 0) {

					for (var i = 0; i < res.json().length; i++) {
						items.push({
							cve: res.json()[i].cve,
							descrip: res.json()[i].descrip,
							cant: res.json()[i].cant,
							modi: res.json()[i].modificado
						});
					}

					//Eliminar los hijos de pendientes
					myTree.deleteChildItems(7);

					//Vuelve a agregar a pendientes
					items.forEach(function (elem) {
						//console.log(elem)
						var modif = "<div class= 'div_pend " + (elem.modi == 0 ? "sin_pend'>" : "con_pend'>") + elem.modi + "</div>";
						var txtCantidad = "<div class= 'div_pend " + (elem.cant == 0 ? "sin_pend'>" : "con_pend'>") + elem.cant + "</div>";
						myTree.insertNewItem(7, "7" + elem.cve, modif + txtCantidad + " (" + elem.cve + ") " + elem.descrip, 0, "building.png", "building.png", "building.png");
					});
					/*for (var i = 0; i < items.length; i++) {
						var elem = items[i];
						var txtCantidad = "<div class= 'div_pend " + (elem.cant == 0 ? "sin_pend'>" : "con_pend'>") + elem.cant + "</div>";
						myTree.insertNewItem(7, "7" + elem.cve, txtCantidad + " (" + elem.cve + ") " + elem.descrip, 0, "building.png", "building.png", "building.png");
					}*/

					ligaURLs(items);

				}
			});

	}


